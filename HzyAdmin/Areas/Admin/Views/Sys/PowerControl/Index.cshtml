@if (!string.IsNullOrEmpty(ViewData["PowerModel"].ToStr()))
{
    <script type="text/javascript">
        var PowerModel=@Html.Raw(ViewData["PowerModel"]);
        var entity=PowerModel;
        $(function(){
            if(!entity.Have){
                admin.msg("缺少【是否拥有该菜单的权限】！","警告");
            }
        });
    </script>
}